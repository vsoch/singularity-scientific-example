@article{PaezEspino:2016gi,
author = {Paez-Espino, David and Eloe-Fadrosh, Emiley A and Pavlopoulos, Georgios A and Thomas, Alex D and Huntemann, Marcel and Mikhailova, Natalia and Rubin, Edward and Ivanova, Natalia N and Kyrpides, Nikos C},
