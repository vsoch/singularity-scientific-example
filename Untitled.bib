journal = {Science},
year = {2016},
volume = {351},
number = {6278},
pages = {1218--1222},
month = mar,
publisher = {American Association for the Advancement of Science},
affiliation = {Verna and Marrs McLean Department of Biochemistry and Molecular Biology, Baylor College of Medicine, Houston, TX 77030, USA. Center for Theoretical Biological Physics, Rice University, Houston, TX 77005, USA.},
