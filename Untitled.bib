}

@article{Archer:2007ds,
author = {Archer, Hayley and Evans, Julie and Leonard, Helen and Colvin, Lyn and Ravine, David and Christodoulou, John and Williamson, Sarah and Charman, Tony and Bailey, Mark E S and Sampson, Julian and de Klerk, Nicholas and Clarke, Angus},
title = {{Correlation between clinical severity in patients with Rett syndrome with a p.R168X or p.T158M MECP2 mutation, and the direction and degree of skewing of X-chromosome inactivation.}},
journal = {Journal of Medical Genetics},
year = {2007},
volume = {44},
number = {2},
pages = {148--152},
month = feb,
publisher = {BMJ Publishing Group Ltd},
affiliation = {Institute of Medical Genetics, Cardiff University, University Hospital of Wales, Cardiff, UK.},
doi = {10.1136/jmg.2006.045260},
pmid = {16905679},
pmcid = {PMC2598067},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-10-12T20:56:23GMT},
date-modified = {2016-01-13T01:14:31GMT},
abstract = {INTRODUCTION:Rett syndrome (RTT) is an X-linked dominant neurodevelopmental disorder that is usually associated with mutations in the MECP2 gene. The most common mutations in the gene are p.R168X and p.T158M. The influence of X-chromosome inactivation (XCI) on clinical severity in patients with RTT with these mutations was investigated, taking into account the extent and direction of skewing.

METHODS:Female patients and their parents were recruited from the UK and Australia. Clinical severity was measured by the Pineda Severity and Kerr profile scores. The degree of XCI and its direction relative to the X chromosome parent of origin were measured in DNA prepared from peripheral blood leucocytes, and allele-specific polymerase chain reaction was used to determine the parental origin of mutation. Combining these, the percentage of cells expected to express the mutant allele was calculated.

RESULTS:Linear regression analysis was undertaken for fully informative cases with p.R168X (n = 23) and p.T158M (n = 20) mutations. A statistically significant increase in clinical severity with increase in the proportion of active mutated allele was shown for both the p.R168X and p.T158M mutations.

CONCLUSIONS:XCI may vary in neurological and haematological tissues. However, these data are the first to show a relationship between the degree and direction of XCI in leucocytes and clinical severity in RTT, although the clinical utility of this in giving a prognosis for individual patients is unclear.},
url = {http://jmg.bmj.com/content/44/2/148.full},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/50/503AA627-5C7A-440D-9DFC-954DDF9E6F0E.pdf},
file = {{503AA627-5C7A-440D-9DFC-954DDF9E6F0E.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/50/503AA627-5C7A-440D-9DFC-954DDF9E6F0E.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1136/jmg.2006.045260}}
}

@article{Morisaki:2016bu,
author = {Morisaki, Tatsuya and Lyon, Kenneth and DeLuca, Keith F and DeLuca, Jennifer G and English, Brian P and Zhang, Zhengjian and Lavis, Luke D and Grimm, Jonathan B and Viswanathan, Sarada and Looger, Loren L and Lionnet, Timoth{\'e}e and Stasevich, Timothy J},
title = {{Real-time quantification of single RNA translation dynamics in living cells.}},
journal = {Science},
year = {2016},
volume = {352},
number = {6292},
pages = {1425--1429},
month = jun,
affiliation = {Department of Biochemistry and Molecular Biology and Institute for Genome Architecture and Function, Colorado State University, Fort Collins, CO, USA.},
doi = {10.1126/science.aaf0899},
pmid = {27313040},
language = {English},
rating = {0},
date-added = {2016-06-23T18:45:00GMT},
date-modified = {2016-08-26T03:34:58GMT},
abstract = {Although messenger RNA (mRNA) translation is a fundamental biological process, it has never been imaged in real time in vivo with single-molecule precision. To achieve this, we developed nascent chain tracking (NCT), a technique that uses multi-epitope tags and antibody-based fluorescent probes to quantify protein synthesis dynamics at the single-mRNA level. NCT reveals an elongation rate of {\textasciitilde}10 amino acids per second, with initiation occurring stochastically every {\textasciitilde}30 seconds. Polysomes contain {\textasciitilde}1 ribosome every 200 to 900 nucleotides and are globular rather than elongated in shape. By developing multicolor probes, we showed that most polysomes act independently; however, a small fraction ({\textasciitilde}5%) form complexes in which two distinct mRNAs can be translated simultaneously. The sensitivity and versatility of NCT make it a powerful new tool for quantifying mRNA translation kinetics.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.aaf0899},
uri = {\url{papers3://publication/doi/10.1126/science.aaf0899}}
}

