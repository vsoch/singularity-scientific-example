@article{EsparzaBaquer:2015ex,
author = {Esparza-Baquer, A and Larruskain, A and Mateo-Abad, M and Minguij{\'o}n, E and Juste, R A and Benavides, J and P{\'e}rez, V and Jugo, B M},
title = {{SNPs in APOBEC3 cytosine deaminases and their association with Visna/Maedi disease progression.}},
journal = {Veterinary immunology and immunopathology},
year = {2015},
volume = {163},
number = {3-4},
pages = {125--133},
month = feb,
affiliation = {Genetics, Physical Anthropology and Animal Physiology Department, Faculty of Science and Technology, University of the Basque Country (UPV/EHU), Leioa, Spain.},
doi = {10.1016/j.vetimm.2014.11.007},
pmid = {25532445},
language = {English},
rating = {0},
date-added = {2015-10-11T18:13:47GMT},
date-modified = {2016-01-13T01:14:26GMT},
abstract = {The Apolipoprotein B mRNA-editing catalytic polypeptide-like 3 (APOBEC3) genes are able to inhibit the replication of a wide range of exogenous retroviruses, as well as endogenous retroviruses and retrotransposons. Three APOBEC3 genes, named APOBEC3Z1, APOBEC3Z2 and APOBEC3Z3, have been described in sheep. In this work the three genes have been screened in order to identify polymorphisms. No polymorphism was detected for the A3Z2 and A3Z3 genes but 16 SNPs and a 3-bp deletion were found in the A3Z1 gene. A thermoestability prediction analysis was applied to the detected amino acidic SNPs by three different programs. This analysis revealed a number of polymorphisms that could affect the protein stability. The SNPs of the 3'UTR were tested to detect alterations on the predicted microRNA target sites. Two new microRNA target sites were discovered for one of the alleles. Two SNPs were selected for association studies in relation with the retroviral disease Visna/Maedi in Latxa and Assaf sheep breeds. Although association analyses resulted unconclusive, probably due to the unsuitability of the SNP allele frequency distribution of the selected polymorphisms in the analyzed breeds, these genes remain good candidates for association studies.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0165242714002645},
uri = {\url{papers3://publication/doi/10.1016/j.vetimm.2014.11.007}}
}

@article{Willems:2015cq,
author = {Willems, Luc and Gillet, Nicolas Albert},
title = {{APOBEC3 Interference during Replication of Viral Genomes.}},
journal = {Viruses},
year = {2015},
volume = {7},
