@article{Rawson:2014bf,
author = {Rawson, Randi L and Yam, Lung and Weimer, Robby M and Bend, Eric G and Hartwieg, Erika and Horvitz, H Robert and Clark, Scott G and Jorgensen, Erik M},
title = {{Axons degenerate in the absence of mitochondria in C. elegans.}},
journal = {Current biology : CB},
year = {2014},
volume = {24},
number = {7},
pages = {760--765},
month = mar,
affiliation = {Department of Biology and Howard Hughes Medical Institute, University of Utah, Salt Lake City, UT 84112-0840, USA.},
doi = {10.1016/j.cub.2014.02.025},
pmid = {24631238},
pmcid = {PMC4018749},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-10-16T16:50:35GMT},
date-modified = {2016-01-13T01:14:34GMT},
abstract = {Many neurodegenerative disorders are associated with mitochondrial defects [1-3]. Mitochondria can play an active role in degeneration by releasing reactive oxygen species and apoptotic factors [4-7]. Alternatively, mitochondria can protect axons from stress and insults, for example by buffering calcium [8]. Recent studies manipulating mitochondria lend support to both of these models [9-13]. Here, we identify a C. elegans mutant, ric-7, in which mitochondria are unable to exit the neuron cell bodies, similar to the kinesin-1/unc-116 mutant. When axons lacking mitochondria are cut with a laser, they rapidly degenerate. Some neurons even spontaneously degenerate in ric-7 mutants. Degeneration can be suppressed by forcing mitochondria into the axons of the mutants. The protective effect of mitochondria is also observed in the wild-type: a majority of axon fragments containing a mitochondrion survive axotomy, whereas those lacking mitochondria degenerate. Thus, mitochondria are not required for axon degeneration and serve a protective role in C. elegans axons.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0960982214001936},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B447E666-07BA-47B2-9BE3-38E4F9F9C89F.pdf},
file = {{B447E666-07BA-47B2-9BE3-38E4F9F9C89F.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B447E666-07BA-47B2-9BE3-38E4F9F9C89F.pdf:application/pdf;B447E666-07BA-47B2-9BE3-38E4F9F9C89F.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B447E666-07BA-47B2-9BE3-38E4F9F9C89F.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.cub.2014.02.025}}
}

