author = {Basen, Mirko and Schut, Gerrit J and Nguyen, Diep M and Lipscomb, Gina L and Benn, Robert A and Prybol, Cameron J and Vaccaro, Brian J and Poole, Farris L and Kelly, Robert M and Adams, Michael W W},
title = {{Single gene insertion drives bioalcohol production by a thermophilic archaeon.}},
