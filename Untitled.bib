pages = {319},
publisher = {Nature Publishing Group},
affiliation = {Department of Biochemistry, University of Utah, 15 N Medical Drive East, Room 4800, Salt Lake City, Utah 84112, USA.},
doi = {10.1038/ncomms1324},
