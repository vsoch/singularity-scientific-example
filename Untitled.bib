pages = {12877--12883},
month = sep,
publisher = {Society for Neuroscience},
affiliation = {Department of Biology and Center for Behavioral Genomics, Brandeis University, Waltham, Massachusetts 02454, and Janelia Farm Research Campus, Ashburn, Virginia 20147 suginok@janelia.hhmi.org nelson@brandeis.edu.},
doi = {10.1523/JNEUROSCI.2674-14.2014},
