title = {{APOBEC1-mediated editing and attenuation of herpes simplex virus 1 DNA indicate that neurons have an antiviral role during herpes simplex encephalitis.}},
journal = {Journal of virology},
year = {2011},
volume = {85},
number = {19},
pages = {9726--9736},
