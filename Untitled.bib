pages = {827--830},
month = aug,
publisher = {American Association for the Advancement of Science},
affiliation = {Department of Genetics and Genomic Sciences, The Icahn Institute for Genomics and Multiscale Biology Icahn School of Medicine at Mount Sinai, One Gustave L. Levy Place, New York , NY 10029, USA. Clinical Gene Networks AB, Jungfrugatan 10, 114 44 Stockholm, Sweden.},
