@article{Zhang:2015fp,
author = {Zhang, Linbin and Sun, Tianai and Woldesellassie, Fitsum and Xiao, Hailian and Tao, Yun},
title = {{Sex ratio meiotic drive as a plausible evolutionary mechanism for hybrid male sterility.}},
journal = {PLoS Genet},
year = {2015},
volume = {11},
number = {3},
pages = {e1005073},
month = mar,
publisher = {Public Library of Science},
affiliation = {Department of Biology, Emory University, Atlanta, Georgia, United States of America.},
doi = {10.1371/journal.pgen.1005073},
pmid = {25822261},
