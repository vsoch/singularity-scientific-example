year = {2014},
volume = {5},
number = {4},
pages = {1064--1094},
publisher = {Multidisciplinary Digital Publishing Institute},
affiliation = {Program in Biochemistry, Cellular and Molecular Biology, Johns Hopkins School of Medicine, Baltimore, MD 21205, USA. freedd@kennedykrieger.org.},
doi = {10.3390/genes5041064},
