pages = {2999--3018},
month = jun,
publisher = {Multidisciplinary Digital Publishing Institute},
affiliation = {Molecular and Cellular Epigenetics, Interdisciplinary Cluster for Applied Genoproteomics (GIGA) of University of Li{\`e}ge (ULg), B34, 1 avenue de L'H{\^o}pital, Sart-Tilman Li{\`e}ge 4000, Belgium. luc.willems@ulg.ac.be.},
