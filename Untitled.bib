author = {Wu, Han and Nord, Alex S and Akiyama, Jennifer A and Shoukry, Malak and Afzal, Veena and Rubin, Edward M and Pennacchio, Len A and Visel, Axel},
title = {{Tissue-specific RNA expression marks distant-acting developmental enhancers.}},
journal = {PLoS Genet},
year = {2014},
