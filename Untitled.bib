title = {{Analysis of genetic inheritance in a family quartet by whole-genome sequencing.}},
journal = {Science},
year = {2010},
volume = {328},
number = {5978},
pages = {636--639},
month = apr,
affiliation = {Institute for Systems Biology, Seattle, WA 98103, USA.},
doi = {10.1126/science.1186802},
pmid = {20220176},
pmcid = {PMC3037280},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-01-12T17:33:24GMT},
date-modified = {2016-06-17T20:22:49GMT},
abstract = {We analyzed the whole-genome sequences of a family of four, consisting of two siblings and their parents. Family-based sequencing allowed us to delineate recombination sites precisely, identify 70% of the sequencing errors (resulting in > 99.999% accuracy), and identify very rare single-nucleotide polymorphisms. We also directly estimated a human intergeneration mutation rate of approximately 1.1 x 10(-8) per position per haploid genome. Both offspring in this family have two recessive disorders: Miller syndrome, for which the gene was concurrently identified, and primary ciliary dyskinesia, for which causative genes have been previously identified. Family-based genome analysis enabled us to narrow the candidate genes for both of these Mendelian disorders to only four. Our results demonstrate the value of complete genome sequencing in families.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.1186802},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/F0/F015F4C6-63E5-4D20-9AC9-961DFEA7AA1C.pdf},
file = {{F015F4C6-63E5-4D20-9AC9-961DFEA7AA1C.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/F0/F015F4C6-63E5-4D20-9AC9-961DFEA7AA1C.pdf:application/pdf;F015F4C6-63E5-4D20-9AC9-961DFEA7AA1C.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/F0/F015F4C6-63E5-4D20-9AC9-961DFEA7AA1C.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.1186802}}
}

@article{Visel:2007ew,
author = {Visel, Axel and Bristow, James and Pennacchio, Len A},
title = {{Enhancer identification through comparative genomics.}},
journal = {Seminars in cell {\&} developmental biology},
year = {2007},
volume = {18},
number = {1},
pages = {140--152},
month = feb,
affiliation = {Genomics Division, MS 84-171, Lawrence Berkeley National Laboratory, Berkeley, CA 94720, USA.},
doi = {10.1016/j.semcdb.2006.12.014},
pmid = {17276707},
pmcid = {PMC1855162},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-09-02T00:40:09GMT},
date-modified = {2016-09-12T22:27:15GMT},
abstract = {With the availability of genomic sequence from numerous vertebrates, a paradigm shift has occurred in the identification of distant-acting gene regulatory elements. In contrast to traditional gene-centric studies in which investigators randomly scanned genomic fragments that flank genes of interest in functional assays, the modern approach begins electronically with publicly available comparative sequence datasets that provide investigators with prioritized lists of putative functional sequences based on their evolutionary conservation. However, although a large number of tools and resources are now available, application of comparative genomic approaches remains far from trivial. In particular, it requires users to dynamically consider the species and methods for comparison depending on the specific biological question under investigation. While there is currently no single general rule to this end, it is clear that when applied appropriately, comparative genomic approaches exponentially increase our power in generating biological hypotheses for subsequent experimental testing. It is anticipated that cardiac-related genes and the identification of their distant-acting transcriptional enhancers are particularly poised to benefit from these modern capabilities.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17276707&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/2A/2AF7140E-6C42-4710-8333-A36C12F3F208.pdf},
file = {{2AF7140E-6C42-4710-8333-A36C12F3F208.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/2A/2AF7140E-6C42-4710-8333-A36C12F3F208.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.semcdb.2006.12.014}}
}

@article{Wu:2016gt,
author = {Wu, Bin and Eliscovich, Carolina and Yoon, Young J and Singer, Robert H},
title = {{Translation dynamics of single mRNAs in live cells and neurons.}},
journal = {Science},
year = {2016},
volume = {352},
number = {6292},
pages = {1430--1435},
month = jun,
affiliation = {Department of Anatomy and Structural Biology, Albert Einstein College of Medicine, Bronx, NY 10461, USA. Gruss-Lipper Biophotonics Center, Albert Einstein College of Medicine, Bronx, NY 10461, USA.},
doi = {10.1126/science.aaf1084},
pmid = {27313041},
language = {English},
rating = {0},
date-added = {2016-06-23T18:44:45GMT},
date-modified = {2016-08-26T03:34:58GMT},
abstract = {Translation is the fundamental biological process converting mRNA information into proteins. Single-molecule imaging in live cells has illuminated the dynamics of RNA transcription; however, it is not yet applicable to translation. Here, we report single-molecule imaging of nascent peptides (SINAPS) to assess translation in live cells. The approach provides direct readout of initiation, elongation, and location of translation. We show that mRNAs coding for endoplasmic reticulum (ER) proteins are translated when they encounter the ER membrane. Single-molecule fluorescence recovery after photobleaching provides direct measurement of elongation speed (5 amino acids per second). In primary neurons, mRNAs are translated in proximal dendrites but repressed in distal dendrites and display "bursting" translation. This technology provides a tool with which to address the spatiotemporal translation mechanism of single mRNAs in living cells.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.aaf1084},
uri = {\url{papers3://publication/doi/10.1126/science.aaf1084}}
}

