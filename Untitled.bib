@article{Davis:2016jm,
author = {Davis, Jennifer and Davis, L Craig and Correll, Robert N and Makarewich, Catherine A and Schwanekamp, Jennifer A and Moussavi-Harami, Farid and Wang, Dan and York, Allen J and Wu, Haodi and Houser, Steven R and Seidman, Christine E and Seidman, Jonathan G and Regnier, Michael and Metzger, Joseph M and Wu, Joseph C and Molkentin, Jeffery D},
