}

@article{Savic:2015ea,
author = {Savic, Daniel and Partridge, E Christopher and Newberry, Kimberly M and Smith, Sophia B and Meadows, Sarah K and Roberts, Brian S and Mackiewicz, Mark and Mendenhall, Eric M and Myers, Richard M},
title = {{CETCh-seq: CRISPR epitope tagging ChIP-seq of DNA-binding proteins.}},
journal = {Genome research},
year = {2015},
month = sep,
publisher = {Cold Spring Harbor Lab},
affiliation = {HudsonAlpha Institute for Biotechnology, Huntsville, Alabama 35806, USA;},
doi = {10.1101/gr.193540.115},
