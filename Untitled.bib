@article{Bonnefond:2013kg,
author = {Bonnefond, Am{\'e}lie and Skrobek, Boris and Lobbens, St{\'e}phane and Eury, Elodie and Thuillier, Doroth{\'e}e and Cauchi, St{\'e}phane and Lantieri, Olivier and Balkau, Beverley and Riboli, Elio and Marre, Michel and Charpentier, Guillaume and Yengo, Lo{\"\i}c and Froguel, Philippe},
title = {{Association between large detectable clonal mosaicism and type 2 diabetes with vascular complications.}},
journal = {Nat Genet},
year = {2013},
volume = {45},
number = {9},
pages = {1040--1043},
month = sep,
affiliation = {Centre National de Recherche Scientifique (CNRS) Unit{\'e} Mixte de Recherche 8199, Lille Pasteur Institute, Lille, France.},
doi = {10.1038/ng.2700},
pmid = {23852171},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-03-19T19:55:01GMT},
date-modified = {2016-06-17T20:22:49GMT},
abstract = {Large chromosomal clonal mosaic events (CMEs) have been suggested to be linked to aging and to predict cancer. Type 2 diabetes (T2D) has been conceptualized as an accelerated-aging disease and is associated with higher prevalence of cancers. Here we aimed to assess the association between T2D and CME occurrence in blood. We evaluated the presence of CMEs in 7,659 individuals (including 2,208 with T2D) using DNA arrays. A significant association between CME occurrence and T2D was found (odds ratio (OR) = 5.3; P = 5.1 {\texttimes} 10(-5)) and was stronger when we only considered non-obese individuals with T2D (OR = 5.6; P = 4.9 {\texttimes} 10(-5)). Notably, CME carriers with T2D had higher prevalence of vascular complications than non-carriers with T2D (71.4% versus 37.1%, respectively; P = 7.7 {\texttimes} 10(-4)). In CME carriers, we found an increase in the percentage of abnormal cells over 6 years (P = 8.60 {\texttimes} 10(-3)). In conclusion, given the increased risk of cancer in CME carriers, our results may have profound clinical implications in patients with severe T2D.},
url = {http://www.nature.com/doifinder/10.1038/ng.2700},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/18/18CDEA5F-CF2F-4DA9-BA53-9C81E94CDC01.pdf},
file = {{18CDEA5F-CF2F-4DA9-BA53-9C81E94CDC01.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/18/18CDEA5F-CF2F-4DA9-BA53-9C81E94CDC01.pdf:application/pdf;18CDEA5F-CF2F-4DA9-BA53-9C81E94CDC01.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/18/18CDEA5F-CF2F-4DA9-BA53-9C81E94CDC01.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/ng.2700}}
}

@article{Bewick:2016hf,
author = {Bewick, Adam J and Ji, Lexiang and Niederhuth, Chad E and Willing, Eva-Maria and Hofmeister, Brigitte T and Shi, Xiuling and Wang, Li and Lu, Zefu and Rohr, Nicholas A and Hartwig, Benjamin and Kiefer, Christiane and Deal, Roger B and Schmutz, Jeremy and Grimwood, Jane and Stroud, Hume and Jacobsen, Steven E and Schneeberger, Korbinian and Zhang, Xiaoyu and Schmitz, Robert J},
title = {{On the origin and evolutionary consequences of gene body DNA methylation.}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2016},
volume = {113},
number = {32},
pages = {201604666--9116},
month = jul,
affiliation = {Department of Genetics, University of Georgia, Athens, GA 30602;},
doi = {10.1073/pnas.1604666113},
