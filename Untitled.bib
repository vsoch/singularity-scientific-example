@article{Peters:2016fk,
author = {Peters, Jason M and Colavin, Alexandre and Shi, Handuo and Czarny, Tomasz L and Larson, Matthew H and Wong, Spencer and Hawkins, John S and Lu, Candy H S and Koo, Byoung-Mo and Marta, Elizabeth and Shiver, Anthony L and Whitehead, Evan H and Weissman, Jonathan S and Brown, Eric D and Qi, Lei S and Huang, Kerwyn Casey and Gross, Carol A},
title = {{A Comprehensive, CRISPR-based Functional Analysis of Essential Genes in Bacteria.}},
journal = {Cell},
year = {2016},
volume = {165},
number = {6},
pages = {1493--1506},
month = jun,
affiliation = {Department of Microbiology and Immunology, University of California, San Francisco, San Francisco, CA 94158, USA.},
doi = {10.1016/j.cell.2016.05.003},
pmid = {27238023},
pmcid = {PMC4894308},
language = {English},
rating = {0},
date-added = {2016-06-13T18:24:25GMT},
date-modified = {2016-08-26T03:34:58GMT},
abstract = {Essential gene functions underpin the core reactions required for cell viability, but their contributions and~relationships are poorly studied in~vivo. Using CRISPR interference, we created knockdowns of every essential gene in Bacillus subtilis and probed their phenotypes. Our high-confidence essential gene network, established using chemical genomics, showed extensive interconnections among distantly related processes and identified modes of action for uncharacterized antibiotics. Importantly, mild knockdown of essential gene functions significantly reduced stationary-phase survival without affecting maximal growth rate, suggesting that essential protein levels are set to maximize outgrowth from stationary phase. Finally, high-throughput microscopy indicated that cell morphology is relatively insensitive to mild knockdown but profoundly affected by depletion of gene function, revealing intimate connections between cell growth and shape. Our results provide a framework for systematic investigation of essential gene functions in~vivo broadly applicable to diverse microorganisms and amenable to comparative analysis.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0092867416305517},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/6F/6FCA0B8A-C4A0-454B-8D80-5D5F49C9D071.pdf},
file = {{6FCA0B8A-C4A0-454B-8D80-5D5F49C9D071.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/6F/6FCA0B8A-C4A0-454B-8D80-5D5F49C9D071.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.cell.2016.05.003}}
}

