file = {{A1D9160A-1EBF-48D4-8898-A85FD4605315.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/A1/A1D9160A-1EBF-48D4-8898-A85FD4605315.pdf:application/pdf;A1D9160A-1EBF-48D4-8898-A85FD4605315.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/A1/A1D9160A-1EBF-48D4-8898-A85FD4605315.pdf:application/pdf}},
