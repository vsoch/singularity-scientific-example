author = {Liu, Siyuan John and Nowakowski, Tomasz J and Pollen, Alex A and Lui, Jan H and Horlbeck, Max A and Attenello, Frank J and He, Daniel and Weissman, Jonathan S and Kriegstein, Arnold R and Diaz, Aaron A and Lim, Daniel A},
title = {{Single-cell analysis of long non-coding RNAs in the developing human neocortex.}},
journal = {Genome Biology},
year = {2016},
volume = {17},
number = {1},
pages = {67},
affiliation = {Department of Neurological Surgery, University of California, San Francisco, Ray and Dagmar Dolby Regeneration Medicine Building, 35 Medical Center Way, RMB 1037, San Francisco, CA, 94143, USA.},
