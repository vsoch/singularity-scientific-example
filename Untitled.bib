@article{Kristiansen:2002bo,
author = {Kristiansen, M and Langer{\o}d, A and Knudsen, G P and Weber, B L and B{\o}rresen-Dale, A-L and {\O}rstavik, K H},
title = {{High frequency of skewed X inactivation in young breast cancer patients.}},
journal = {Journal of Medical Genetics},
year = {2002},
volume = {39},
number = {1},
pages = {30--33},
month = jan,
publisher = {BMJ Publishing Group Ltd},
affiliation = {Institute of Medical Genetics, University of Oslo, Oslo, Norway.},
doi = {10.1136/jmg.39.1.30},
pmid = {11826021},
pmcid = {PMC1734952},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-10-12T20:56:23GMT},
date-modified = {2016-01-13T01:14:31GMT},
abstract = {INTRODUCTION:Patients with invasive ovarian cancer were recently shown to have a higher frequency of skewed X chromosome inactivation in peripheral blood cells compared to patients with borderline cancer and controls. In this study, we analysed the X inactivation pattern in peripheral blood from 216 breast cancer patients.

METHODS:X inactivation analysis was performed using HpaII predigestion of DNA followed by PCR of the highly polymorphic CAG repeat of the androgen receptor gene (AR), which amplifies the undigested inactive X chromosome only. The X inactivation pattern was classified as skewed when 90% or more of the cells preferentially used one X chromosome.

RESULTS:Young breast cancer patients (27-45 years) had a higher frequency of skewed X inactivation than young controls (13 and 1%, respectively) (p=0.009), whereas no difference was found for middle aged and older patients compared to controls of a similar age.

CONCLUSIONS:A germline mutation in an X linked tumour suppressor gene may give a proliferative advantage to cells with this mutation on the active X chromosome, thus causing skewed X inactivation and an increased risk for developing cancer. Another possible explanation could be that females with a constitutionally skewed X inactivation pattern are more susceptible to develop breast cancer because of an X linked low penetrance susceptibility allele that is affected by the inactivation pattern.},
url = {http://jmg.bmj.com/content/39/1/30.full},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B4230587-20C0-44A1-962D-40175606E816.pdf},
file = {{B4230587-20C0-44A1-962D-40175606E816.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B4230587-20C0-44A1-962D-40175606E816.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1136/jmg.39.1.30}}
}

