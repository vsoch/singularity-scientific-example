file = {{FF7A6F6A-A0FC-409B-9333-8F79FC3261E0.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/FF/FF7A6F6A-A0FC-409B-9333-8F79FC3261E0.pdf:application/pdf;FF7A6F6A-A0FC-409B-9333-8F79FC3261E0.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/FF/FF7A6F6A-A0FC-409B-9333-8F79FC3261E0.pdf:application/pdf}},
