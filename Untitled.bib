title = {{Human APOBEC3 induced mutation of human immunodeficiency virus type-1 contributes to adaptation and evolution in natural infection.}},
journal = {PLoS pathogens},
year = {2014},
volume = {10},
number = {7},
pages = {e1004281},
month = jul,
publisher = {Public Library of Science},
affiliation = {Division of Infectious Diseases, Northwestern University Feinberg School of Medicine, Chicago, Illinois, United States of America.},
