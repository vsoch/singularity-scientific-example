@article{Cao:2016bt,
author = {Cao, Nan and Huang, Yu and Zheng, Jiashun and Spencer, C Ian and Zhang, Yu and Fu, Ji-Dong and Nie, Baoming and Xie, Min and Zhang, Mingliang and Wang, Haixia and Ma, Tianhua and Xu, Tao and Shi, Guilai and Srivastava, Deepak and Ding, Sheng},
title = {{Conversion of human fibroblasts into functional cardiomyocytes by small molecules.}},
journal = {Science},
year = {2016},
month = apr,
affiliation = {Gladstone Institute of Cardiovascular Disease, San Francisco, CA 94158, USA. Department of Pharmaceutical Chemistry, University of California, San Francisco, San Francisco, CA 94158, USA.},
doi = {10.1126/science.aaf1502},
pmid = {27127239},
language = {English},
rating = {0},
date-added = {2016-05-13T17:21:39GMT},
date-modified = {2016-07-08T02:32:16GMT},
abstract = {Reprogramming somatic fibroblasts into alternative lineages would provide a promising source of cells for regenerative therapy. However, transdifferentiating human cells to specific homogeneous, functional cell types is challenging. Here we show that cardiomyocyte-like cells can be generated by treating human fibroblasts with a combination of nine compounds (9C). The chemically induced cardiomyocyte-like cells (ciCMs) uniformly contracted and resembled human cardiomyocytes in their transcriptome, epigenetic, and electrophysiological properties. 9C treatment of human fibroblasts resulted in a more open-chromatin conformation at key heart developmental genes, enabling their promoters/enhancers to bind effectors of major cardiogenic signals. When transplanted into infarcted mouse hearts, 9C-treated fibroblasts were efficiently converted to ciCMs. This pharmacological approach for lineage-specific reprogramming may have many important therapeutic implications after further optimization to generate mature cardiac cells.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.aaf1502},
uri = {\url{papers3://publication/doi/10.1126/science.aaf1502}}
}

@article{Freed:2014ka,
author = {Freed, Donald and Stevens, Eric L and Pevsner, Jonathan},
title = {{Somatic mosaicism in the human genome.}},
