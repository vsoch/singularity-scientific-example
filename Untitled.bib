title = {{RNA editing by ADAR1 prevents MDA5 sensing of endogenous dsRNA as nonself.}},
journal = {Science},
year = {2015},
volume = {349},
number = {6252},
pages = {1115--1120},
month = sep,
publisher = {American Association for the Advancement of Science},
affiliation = {St. Vincent's Institute of Medical Research, Fitzroy, Victoria 3065, Australia. Department of Medicine, St. Vincent's Hospital, University of Melbourne, Fitzroy, Victoria 3065, Australia.},
doi = {10.1126/science.aac7049},
pmid = {26275108},
language = {English},
read = {Yes},
rating = {0},
date-added = {2015-09-27T18:30:04GMT},
date-modified = {2015-12-02T17:09:43GMT},
abstract = {Adenosine-to-inosine (A-to-I) editing is a highly prevalent posttranscriptional modification of RNA, mediated by ADAR (adenosine deaminase acting on RNA) enzymes. In addition to RNA editing, additional functions have been proposed for ADAR1. To determine the specific role of RNA editing by ADAR1, we generated mice with an editing-deficient knock-in mutation (Adar1(E861A), where E861A denotes Glu(861){\textrightarrow}Ala(861)). Adar1(E861A/E861A) embryos died at {\textasciitilde}E13.5 (embryonic day 13.5), with activated interferon and double-stranded RNA (dsRNA)-sensing pathways. Genome-wide analysis of the in vivo substrates of ADAR1 identified clustered hyperediting within long dsRNA stem loops within 3' untranslated regions of endogenous transcripts. Finally, embryonic death and phenotypes of Adar1(E861A/E861A) were rescued by concurrent deletion of the cytosolic sensor of dsRNA, MDA5. A-to-I editing of endogenous dsRNA is the essential function of ADAR1, preventing the activation of the cytosolic dsRNA response by endogenous transcripts.},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.aac7049},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/B1/B13CC8A8-8572-477C-860F-58999DB80E8D.pdf},
file = {{B13CC8A8-8572-477C-860F-58999DB80E8D.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B1/B13CC8A8-8572-477C-860F-58999DB80E8D.pdf:application/pdf;B13CC8A8-8572-477C-860F-58999DB80E8D.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B1/B13CC8A8-8572-477C-860F-58999DB80E8D.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1126/science.aac7049}}
}

@article{Hung:2011hz,
author = {Hung, Tiffany and Wang, Yulei and Lin, Michael F and Koegel, Ashley K and Kotake, Yojiro and Grant, Gavin D and Horlings, Hugo M and Shah, Nilay and Umbricht, Christopher and Wang, Pei and Wang, Yu and Kong, Benjamin and Langer{\o}d, Anita and B{\o}rresen-Dale, Anne-Lise and Kim, Seung K and van de Vijver, Marc and Sukumar, Saraswati and Whitfield, Michael L and Kellis, Manolis and Xiong, Yue and Wong, David J and Chang, Howard Y},
title = {{Extensive and coordinated transcription of noncoding RNAs within cell-cycle promoters.}},
journal = {Nat Genet},
year = {2011},
volume = {43},
number = {7},
pages = {621--629},
month = jul,
annote = {Used poly-A lncRNAs. Showed to focus on 10kb upstream, introns. Can probably ignore downstream, but I'll miss a few hits. Should confirm transcript expression with orthogonal ChIP-seq. Neighboring ncRNAs tend to be co-regulated, independant of nearby genes. Note that inverse correlation is still ok.},
publisher = {Nature Research},
affiliation = {Program in Epithelial Biology, Stanford University School of Medicine, Stanford, California, USA.},
doi = {10.1038/ng.848},
pmid = {21642992},
pmcid = {PMC3652667},
language = {English},
read = {Yes},
rating = {0},
date-added = {2016-08-11T20:53:52GMT},
date-modified = {2016-09-05T20:06:05GMT},
abstract = {Transcription of long noncoding RNAs (lncRNAs) within gene regulatory elements can modulate gene activity in response to external stimuli, but the scope and functions of such activity are not known. Here we use an ultrahigh-density array that tiles the promoters of 56 cell-cycle genes to interrogate 108 samples representing diverse perturbations. We identify 216 transcribed regions that encode putative lncRNAs, many with RT-PCR-validated periodic expression during the cell cycle, show altered expression in human cancers and are regulated in expression by specific oncogenic stimuli, stem cell differentiation or DNA damage. DNA damage induces five lncRNAs from the CDKN1A promoter, and one such lncRNA, named PANDA, is induced in a p53-dependent manner. PANDA interacts with the transcription factor NF-YA to limit expression of pro-apoptotic genes; PANDA depletion markedly sensitized human fibroblasts to apoptosis by doxorubicin. These findings suggest potentially widespread roles for promoter lncRNAs in cell-growth control.},
url = {http://www.nature.com/doifinder/10.1038/ng.848},
local-url = {file://localhost/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B420B546-12A1-433F-B0AA-04C756D77D03.pdf},
file = {{B420B546-12A1-433F-B0AA-04C756D77D03.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B420B546-12A1-433F-B0AA-04C756D77D03.pdf:application/pdf;B420B546-12A1-433F-B0AA-04C756D77D03.pdf:/Users/Cameron/Box/MyFiles/Library.papers3/Files/B4/B420B546-12A1-433F-B0AA-04C756D77D03.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/ng.848}}
}

