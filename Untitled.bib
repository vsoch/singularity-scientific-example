@article{Arribere:2016io,
author = {Arribere, Joshua A and Cenik, Elif S and Jain, Nimit and Hess, Gaelen T and Lee, Cameron H and Bassik, Michael C and Fire, Andrew Z},
title = {{Translation readthrough mitigation.}},
journal = {Nature},
year = {2016},
month = jun,
publisher = {Nature Publishing Group},
affiliation = {Department of Pathology, Stanford University School of Medicine, Stanford, California 94305, USA.},
doi = {10.1038/nature18308},
