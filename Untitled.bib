number = {6248},
pages = {636--638},
month = aug,
affiliation = {Microsoft Research, Mountain View, CA 94043, USA. dwork@microsoft.com vitaly@post.harvard.edu m@mrtz.org toni@cs.toronto.edu omer.reingold@gmail.com aaroth@cis.upenn.edu.},
doi = {10.1126/science.aaa9375},
